{
  "name"            : "node-airbrake-logger"
, "version"         : "0.0.5"
, "description"     : "A simple logger for node.js apps backed by http://airbrake.io/"
, "homepage"        : "http://github.com/goodeggs/node-airbrake-logger"
, "repository"      : { "type" : "git"
                      , "url"  : "https://github.com/goodeggs/node-airbrake-logger.git"
                      }
, "keywords"        : ["logging", "airbrake"]
, "author"          : "Bob Zoller <bob@goodeggsinc.com>"
, "contributors"    : [ "Randy Puro <randy@goodeggsinc.com>"
                      , "Alon Salant <alon@goodeggsinc.com>"
                      ]
, "maintainers"     : "Bob Zoller <bob@goodeggsinc.com>"
, "licenses"        : ["MIT"]
, "dependencies"    : { "airbrake": "0.2.x"
                      }
, "devDependencies" : { "jasmine-node" : "1.0.x"
                      , "coffee-script": "1.2.x"
                      }
, "main"            : "lib/logger"
, "scripts"         : { "pretest" : "coffee -cbl -o lib src"
                      , "test" : "jasmine-node --coffee --junitreport spec"
                      }
}
